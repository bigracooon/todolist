{
    "version": "2.0.0",
    "name": "balashov/checker",
    "description": "This is package for check you project for security, tests and more.",
    "type": "library",
    "authors": [
        {
            "name": "Balashov Ivan"
        }
    ],
    "require": {
        "fakerphp/faker": "^1.23",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^11.2",
        "ramsey/uuid": "*",
        "spatie/phpunit-snapshot-assertions": "^5.1",
        "qossmic/deptrac": "^2.0",
        "squizlabs/php_codesniffer": "^3.10"
    },
    "bin": [
        "bin/check"
    ],
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
